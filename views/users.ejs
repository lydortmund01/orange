<% users.forEach(function(users, index) { 
	console.log(users);
  if (index % 3 == 0) { %>
    <div class="row">
<%} %>
  <div class="span4">
    <h2><a href="/u/<%= users.name %>">
    <p><small><%= users.name %></small></p>
    <p><small><%= users.password %></small></p>
  </div>
<% if (index % 3 == 2) { %>
    </div><!-- end row -->
<% } %>
<%}) %>
<% if (users.length % 3 != 0) { %>
  </div><!-- end row -->
<% } %>